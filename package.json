{
  "name": "cd2-camera-integrator",
  "description": "Componente de câmera universal para React. Projetado com foco em Android, câmeras iOS e webcams padrão.",
  "version": "1.2.2",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "engines": {
    "node": "18.x"
  },
  "keywords": [
    "camera",
    "webcam",
    "cam",
    "react",
    "ios",
    "android",
    "mobile",
    "webRTC"
  ],
  "author": "Bruno Santos <bruno.santos@cd2.com.br>",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@types/webrtc": "0.0.25",
    "axios": "^1.3.4",
    "cd2-camera-integrator": "^1.2.2",
    "detectrtc": "^1.3.9",
    "express": "^4.18.2",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "babel-loader": "^9.1.2",
    "concurrently": "^5.0.2",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "install-peers-cli": "^2.2.0",
    "prepend-file": "^1.3.1",
    "prettier": "^2.7.1",
    "rollup": "^1.29.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-node": "^8.6.2",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.2.1"
  },
  "scripts": {
    "build": "rollup -c",
    "build-watch": "rollup -c -w",
    "prepublishOnly": "npm run build",
    "prepare": "install-peers -f && cd example && npm i",
    "start-example": "cd example && npm start",
    "deploy-example": "cd example && npm run deploy",
    "dev": "concurrently --kill-others \"npm run build-watch\" \"npm run start-example\"",
    "test": "ts-node test/test.ts",
    "pretest": "npm run build",
    "test:lint": "eslint ./src/**/*.ts ./src/**/*.tsx ./example/src/**/*.ts ./example/src/**/*.tsx",
    "test:lint:fix": "npm run test:lint -- --fix"
  },
  "files": [
    "dist"
  ]
}
